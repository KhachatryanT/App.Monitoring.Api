{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo:3": {
      "Name": "Console",
      "Args": {
        "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
      }
    },
    "WriteTo:4": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/verbose/verbose_.log",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 2
              }
            }
          ]
        }
      }
    }
  }
}
